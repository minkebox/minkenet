	phyInfTable OBJECT-TYPE 
		SYNTAX      SEQUENCE OF PhyInfEntry
		MAX-ACCESS  not-accessible 
		STATUS      current 
		DESCRIPTION 
			"A table to control the port specific parameters of the device like speed, duplex mode, etc."
		::= { sysPhyInf 1 }
	   
	phyInfEntry OBJECT-TYPE 
		SYNTAX      PhyInfEntry
		MAX-ACCESS  not-accessible 
		STATUS      current 
		DESCRIPTION 
			"An entry appears in this table for each interface in the system.
			Index to the table is the interface index of the port."
		INDEX { phyInfIndex, phyInfMediaType }
		::= { phyInfTable 1 }
			   
	PhyInfEntry ::= SEQUENCE 
	{ 
		phyInfIndex				Integer32,
		phyInfMediaType			INTEGER,
		phyInfLinkStatus    	INTEGER,
		phyInfMode				INTEGER,
		phyInfDuplex			INTEGER,
		phyInfSpeed				INTEGER,
		phyInfFlowCtrl			INTEGER,
		phyInfFlowCtrlOper		INTEGER,
		phyInfAdminStatus		INTEGER,
		phyInfJumboFrameEnable	INTEGER, 
		phyInfEAPPassThrough	INTEGER, 
		phyInfBPDUPassThrough	INTEGER,
		phyInfportSetDescription  DisplayString
	}

	phyInfIndex OBJECT-TYPE 
		SYNTAX      Integer32  (1..28)
		MAX-ACCESS  read-only 
		STATUS      current 
		DESCRIPTION 
			"Interface index of the port for which the configuration
			in this entry applies."
		::= { phyInfEntry 1 }
	
	phyInfMediaType OBJECT-TYPE 
		SYNTAX	INTEGER  {
			copper(1),
			fiber(2)
		}
		MAX-ACCESS  read-only 
		STATUS      current 
		DESCRIPTION 
			"Port media type Copper(1),  Fiber(2)."
	   ::= { phyInfEntry 2 }

	phyInfLinkStatus OBJECT-TYPE
		SYNTAX	INTEGER  {
					up(1),
					down(2)
				}
		MAX-ACCESS        read-only
		STATUS            current
		DESCRIPTION
			"this is the Link Status of port on the web"
		::= { phyInfEntry 3 }
		
	phyInfMode OBJECT-TYPE 
		SYNTAX	INTEGER {
					auto(1),
					noNegotiation(2)
				}
		MAX-ACCESS  read-write 
		STATUS      current 
		DESCRIPTION 
			"Speficies the mode in which the speed, duplex modes and flow control
			of the interface is determined.
 	
			If set as 'auto', the hardware senses speed and negotiates with the port
			on the other end of the link for data transfer operation as
			'full-duplex' or 'half-duplex' and about flow contol.
 	
			If set as 'nonegotiation', the configured values for interface
			speed, duplex mode and flow control will be effective."
		::= { phyInfEntry 4 }

	phyInfDuplex OBJECT-TYPE 
		SYNTAX	INTEGER {
					full(1),
					half(2)
				}
		MAX-ACCESS  read-write 
		STATUS      current 
		DESCRIPTION 
			"Configures interface data transfer mode as full-duplex or half-duplex.
			This object can be configured only if the 'phyInfMode' is 'nonegotiation'.
			If the 'phyInfMode' is 'auto', it obtains the value from Hardware
			after negotiating  with its peer"
		::= { phyInfEntry 5 }

	phyInfSpeed OBJECT-TYPE 
		SYNTAX	INTEGER {
					hundredMBPS(2),
					oneGB(3),
					tenGB(4)
				}
		MAX-ACCESS  read-write 
		STATUS      current 
		DESCRIPTION 
			"Configures interface speed.
			This object can be configured only if the 'phyInfMode' is 'nonegotiation'.
			If the 'phyInfMode' is 'auto', it obtains the value from Hardware
			after negotiating  with its peer
			Note: tenMBPS is not supported in current product"
		::= { phyInfEntry 6 }

	phyInfFlowCtrl OBJECT-TYPE 
		SYNTAX	INTEGER {
					enabled(1),
					disabled(2)
				  }
		MAX-ACCESS  read-write 
		STATUS      current 
		DESCRIPTION 
			"Enables / disables flow control for the interface.
			This object be configured only if the 'phyInfMode' is 'nonegotiation'.
			If the 'phyInfMode' is 'auto', it obtains the value from Hardware 
			after negotiating  with its peer"
		::= { phyInfEntry 7 }
		
	phyInfFlowCtrlOper OBJECT-TYPE 
		SYNTAX	INTEGER {
					enabled(1),
					disabled(2)
				  }
		MAX-ACCESS  read-only 
		STATUS      current 
		DESCRIPTION 
			"Enables / disables flow control for the interface.
			This object be configured only if the 'phyInfMode' is 'nonegotiation'.
			If the 'phyInfMode' is 'auto', it obtains the value from Hardware 
			after negotiating  with its peer"
		::= { phyInfEntry 8}
		
	phyInfAdminStatus OBJECT-TYPE 
		SYNTAX	INTEGER {
					enabled(1),
					disabled(2)
				  }
		MAX-ACCESS  read-write 
		STATUS      current 
		DESCRIPTION 
			"the port configuration status."
	   ::= { phyInfEntry 9 }

	phyInfJumboFrameEnable OBJECT-TYPE 
		SYNTAX	INTEGER {
					enabled(1),
					disabled(2)
				  }
		MAX-ACCESS  read-write 
		STATUS      current 
		DESCRIPTION 
			"Gigabit Web Smart Switches support jumbo frames (frames larger than 
			the Ethernet frame size of 1522 bytes) of up to 10,000 bytes (tagged). 
			Default jumbo frame is disabled."
		::= { phyInfEntry 10 }

	phyInfEAPPassThrough OBJECT-TYPE 
		SYNTAX	INTEGER {
					enabled(1),
					disabled(2)
				 }
		MAX-ACCESS  read-write
		STATUS      current 
		DESCRIPTION 
			"Enables / disables dot1x EAP packet pass through for the interface.
			This object be configured only if the dot1x is disabled."
		::= { phyInfEntry 11 }

	phyInfBPDUPassThrough     OBJECT-TYPE
		SYNTAX	INTEGER  {
					enabled(1),
					disabled(2)
				}
		MAX-ACCESS        read-write
		STATUS            current
		DESCRIPTION
			"Enables / disables STP BPDU passthrough for the interface.
			This object be configured only if the RSTP/MSTP is disabled."
		::= { phyInfEntry 12 }

